{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "next", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "semi": ["error", "never"]
    // "no-unused-vars": [
    //   "error",
    //   {
    //     "argsIgnorePattern": "^_",
    //     "varsIgnorePattern": "^_"
    //   }
    // ]
  }
}
